{
  "name": "vscode-like-nvim-snippets",
  "author": "Sharma, Vikas",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": ["go"],
        "path": "go.json"
      },
      {
        "language": ["quarto", "markdown"],
        "path": "markdown.json"
      },
      {
        "language": ["fortran", "markdown"],
        "path": "fortran.json"
      },
      {
        "language": "cmake",
        "path": "cmake.json"
      },
      {
        "language": "tex",
        "path": "latex.json"
      }
    ]
  }
}
