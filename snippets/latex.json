{
  "lstinline": {
    "prefix": "lst",
    "description": "lstinline| |",
    "body": "\\lstinline|${name}| $0"
  },
  "code": {
    "prefix": "code",
    "description": "code block",
    "body": [
      "\\lstset{style=fort}",
      "\\begin{lstlisting}",
      "$1",
      "\\end{lstlisting}",
      "\\lstset{style=kw}",
      "",
      "$0"
    ]
  },
  "example": {
    "prefix": "ex",
    "description": "example block",
    "body": [
      "\\begin{example}",
      "\\begin{itemize}",
      "\\item $0",
      "\\end{itemize}",
      "\\end{example}"
    ]
  },
  "Align(ed)": {
    "prefix": "ali",
    "body": [
      "\\begin{align}",
      "\t$0",
      "\\end{align}"
    ],
    "description": "Align(ed)"
  },
  "Cases": {
    "prefix": "cas",
    "body": [
      "\\begin{cases}",
      "\t${1:equation}, &\\text{ if }${2:case}\\\\\\\\",
      "\t$0",
      "\\end{cases}"
    ],
    "description": "Cases"
  },
  "Chapter": {
    "prefix": "cha",
    "body": [
